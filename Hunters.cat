    <selectionEntry id="db9c-8a0d-ea7b-c880" name="Chris, Leader of the Hunters" hidden="false" collective="false" type="model">
      <profiles>
        <profile id="a81c-29b2-925c-bc8a" name="Chris" hidden="false" profileTypeId="40e9-9288-6f65-70a9" profileTypeName="Character">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Type" characteristicTypeId="d9ce-b162-4bd7-6736" value="Tactician"/>
            <characteristic name="Melee" characteristicTypeId="fc7e-dccd-5244-3798" value="2Red"/>
            <characteristic name="Shoot" characteristicTypeId="b227-fd8f-ea1c-65f7" value="1White"/>
            <characteristic name="Defense" characteristicTypeId="ca1d-2465-3eb7-9bd8" value="1White,1Red"/>
            <characteristic name="Health" characteristicTypeId="5e5d-73d3-ba6a-8396" value="5"/>
            <characteristic name="SP" characteristicTypeId="cae4-668b-3f49-b130" value="1White/-"/>
          </characteristics>
        </profile>
      </profiles>
      <rules>
        <rule id="99a1-213b-2a59-7d71" name="A Man&apos;s Gotta Eat" hidden="false">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <description>Chris adds 1Red to all attack rolls against enemy Survivors with 2 or fewer Health points remaining.</description>
        </rule>
        <rule id="99a0-826e-c82a-2657" name="Ugly Business (Special Order)" hidden="false">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <description>Spend 1SP to automatically remove a Prone Survivor (friend or foe) within the Kill zone of Chris or a friendly Hunter in line of sight of Chris. If the Survivor removed is an enemy, Chris adds 1Red to his next Attack roll. If the Survivor was a friendly model, Chris adds 1Red to his Strategy rating in the next Strategy phase. These bonuses are not cumulative (the order may be used to remove multiple Survivors from play, but Chris may only benefit from one attack or Strategy rating bonus).</description>
        </rule>
      </rules>
