    <selectionEntry id="928e-764a-7b21-761b" name="Group Special Rules" hidden="false" collective="false" type="upgrade">
      <profiles/>
      <rules>
        <rule id="1b89-515c-6ecd-c89e" name="Easier Game" hidden="false">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <description>All members of the Hunters faction ignore the Just a Kid keyword.</description>
        </rule>
        <rule id="a2e5-cea1-8ea5-53b4" name="Pick Them Off" hidden="false">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <description>Each time a model from the Hunters faction wounds an enemy Survivor at Medium or Long range, but does not remove them as a casualty, they may spend 1SP to make them Prone.</description>
        </rule>
      </rules>
