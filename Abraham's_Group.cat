    <selectionEntry id="1a9d-9fe9-be6f-ed73" name="Abraham Ford, Army Sergeant" hidden="false" collective="false" type="model">
      <profiles>
        <profile id="d415-b954-c157-0acd" name="Abraham Ford" hidden="false" profileTypeId="40e9-9288-6f65-70a9" profileTypeName="Character">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Type" characteristicTypeId="d9ce-b162-4bd7-6736" value="Tactician"/>
            <characteristic name="Melee" characteristicTypeId="fc7e-dccd-5244-3798" value="1White,1Red"/>
            <characteristic name="Shoot" characteristicTypeId="b227-fd8f-ea1c-65f7" value="1White,1Red"/>
            <characteristic name="Defense" characteristicTypeId="ca1d-2465-3eb7-9bd8" value="1Blue"/>
